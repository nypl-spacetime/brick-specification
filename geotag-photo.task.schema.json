{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "url": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "date": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "image_urls": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string"
          },
          "size": {
            "type": "integer"
          }
        },
        "required": [
          "url"
        ]
      }
    }
  },
  "required": [
    "image_urls"
  ]
}